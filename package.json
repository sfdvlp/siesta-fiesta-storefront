{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build:cf": "cross-env CF_PAGES=1 remix build",
    "build": "tailwindcss -m -i ./tailwind.css -o app/styles/app.css && remix build",
    "dev:wrangler": "wrangler pages dev ./public",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:cf": "npm run build:cf && concurrently \"npm run dev:css\" \"remix watch\" \"npm run dev:wrangler\"",
    "start:cf": "cross-env NODE_ENV=production yarn run dev:wrangler",
    "start": "remix-serve build",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript-generic-sdk": "^2.3.1",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@netlify/functions": "^1.0.0",
    "@remix-run/cloudflare": "^1.6.5",
    "@remix-run/cloudflare-pages": "^1.6.5",
    "@remix-run/netlify": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@remix-run/server-runtime": "1.6.5",
    "@stripe/react-stripe-js": "^1.9.0",
    "@stripe/stripe-js": "^1.32.0",
    "@tailwindcss/forms": "^0.5.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.4.0",
    "graphql-tag": "^2.12.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.6.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.8.1",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@remix-run/dev": "^1.6.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.16",
    "typescript": "^4.1.2",
    "wrangler": "^2.0.6"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
